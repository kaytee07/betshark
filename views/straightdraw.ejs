<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Betsolution</title>
    <link rel="stylesheet" href="/home.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.3.0/font/bootstrap-icons.css">
</head>
<body>
   <div class="app">
    <div class="nav">
        <div class="head">
            <h1>Bet<span class="extendo">SHARK</span></h1>
        </div>  
        <% if(info.user_id){%>  
        <div class="logout">
            <form method="GET" action="/home">
                <button type="submit" class="logout" class="back">back</button>
            </form>   
        </div>  
        <% } %> 
    </div>
    <div class="images">
        <% for(let img of info.image) {%>
            <div class="image">
                <img class="odd_img" src="<%= img.image[0].url %>" alt="">
            </div>
            <form class="del_img" method="POST" action="/delete/straightdraw">
                <input class="invisible" type="text" name="<%= img.image[0].filename %>">
                <% if(info.user_id){%>  
                    <button type="submit">delete</button>
                    <% } %> 
            </form>
        <% } %>  
    </div>
  
   </div>    

   <script src="/app.js"></script>
</body>
</html>